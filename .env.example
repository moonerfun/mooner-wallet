# Your Turnkey organization ID (found in Turnkey dashboard)
EXPO_PUBLIC_TURNKEY_ORGANIZATION_ID=

# Turnkey API base URL (default: https://api.turnkey.com)
EXPO_PUBLIC_TURNKEY_API_BASE_URL=https://api.turnkey.com

# Turnkey auth proxy URL for passkey authentication
EXPO_PUBLIC_TURNKEY_AUTH_PROXY_URL=https://authproxy.turnkey.com

# Auth proxy configuration ID (from Turnkey dashboard)
EXPO_PUBLIC_TURNKEY_AUTH_PROXY_CONFIG_ID=

# Relying Party ID for WebAuthn/Passkey (must match your domain)
EXPO_PUBLIC_TURNKEY_RPID=

# App deep link scheme (used for OAuth callbacks)
EXPO_PUBLIC_APP_SCHEME=moonerwallet

# Supabase project URL
EXPO_PUBLIC_SUPABASE_URL=

# Supabase anonymous/public key (safe for client-side use)
EXPO_PUBLIC_SUPABASE_ANON_KEY=

# Twitter/X OAuth 2.0 Client ID (from X Developer Portal)
EXPO_PUBLIC_X_CLIENT_ID=


# https://docs.onebalance.io
EXPO_PUBLIC_ONEBALANCE_API_KEY=

# https://docs.mobula.io
EXPO_PUBLIC_MOBULA_API_KEY=
EXPO_PUBLIC_MOBULA_API_URL=https://api.mobula.io

# Blockchain RPC Endpoints

# Solana RPC (Helius - recommended for reliability)
EXPO_PUBLIC_SOLANA_RPC_URL=

# EVM Chain RPCs
EXPO_PUBLIC_ETH_RPC_URL=
EXPO_PUBLIC_BASE_RPC_URL=
EXPO_PUBLIC_BSC_RPC_URL=